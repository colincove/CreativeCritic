package com.example.creativecritic;

public class User {
	private String email;
	private String device;
	private String name;
	private int id;
	public User(int id, String email, String name) {
		// TODO Auto-generated constructor stub
		this.id=id;
	}
	public User(int id, String device) {
		// TODO Auto-generated constructor stub
		this.id=id;
		this.device=device;
	}

}
